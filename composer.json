{
    "name": "makeitlv/boilerplate",
    "description": "Project boilerplate",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^8.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "composer/package-versions-deprecated": "1.11.99.1",
        "doctrine/doctrine-bundle": "^2.3",
        "doctrine/doctrine-migrations-bundle": "^3.1",
        "doctrine/orm": "^2.8",
        "jaybizzle/crawler-detect": "^1.2",
        "sensio/framework-extra-bundle": "^5.6",
        "symfony/amqp-messenger": "5.3.*",
        "symfony/cache": "5.3.*",
        "symfony/console": "5.3.*",
        "symfony/css-selector": "5.3.*",
        "symfony/doctrine-messenger": "5.3.*",
        "symfony/dotenv": "5.3.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "5.3.*",
        "symfony/framework-bundle": "5.3.*",
        "symfony/http-client": "5.3.*",
        "symfony/intl": "5.3.*",
        "symfony/mailer": "5.3.*",
        "symfony/messenger": "5.3.*",
        "symfony/monolog-bundle": "^3.7",
        "symfony/proxy-manager-bridge": "5.3.*",
        "symfony/security-bundle": "5.3.*",
        "symfony/security-csrf": "5.3.*",
        "symfony/stopwatch": "5.3.*",
        "symfony/twig-bundle": "5.3.*",
        "symfony/uid": "5.3.*",
        "symfony/validator": "5.3.*",
        "symfony/web-profiler-bundle": "5.3.*",
        "symfony/webpack-encore-bundle": "^1.11",
        "symfony/yaml": "5.3.*",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0"
    },
    "require-dev": {
        "brianium/paratest": "^6.3",
        "phpstan/phpstan": "^0.12.57",
        "phpstan/phpstan-doctrine": "^0.12.25",
        "phpstan/phpstan-strict-rules": "^0.12.5",
        "phpstan/phpstan-symfony": "^0.12.11",
        "roave/security-advisories": "dev-master",
        "sebastian/phpcpd": "^6.0",
        "squizlabs/php_codesniffer": "4.0.x-dev",
        "symfony/browser-kit": "5.3.*",
        "symfony/debug-bundle": "^5.3",
        "symfony/maker-bundle": "^1.33",
        "symfony/phpunit-bridge": "^5.3",
        "symplify/phpstan-rules": "^9.2"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\Common\\": "src/common",
            "App\\Admin\\": "src/admin",
            "App\\Api\\": "src/api",
            "App\\": "src/",
            "App\\AdminCore\\": "src/AdminCore",
            "App\\AdminCache\\": "src/AdminCache",
            "App\\AdminSecurity\\": "src/AdminSecurity",
            "App\\AdminDashboard\\": "src/AdminDashboard",
            "App\\AdminLanguage\\": "src/AdminLanguage",
            "App\\Core\\": "src/Core",
            "App\\ErrorPage\\": "src/ErrorPage"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        },
        "files": ["src/helpers.php"]
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.3.*"
        }
    }
}
